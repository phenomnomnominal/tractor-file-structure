{
  "name": "tractor-file-structure",
  "version": "0.5.1",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-file-structure.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-file-structure/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "test:debug": "mocha --inspect --debug-brk --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.7.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.5.3",
    "npm-watch": "^0.2.0",
    "sinon": "^3.3.0",
    "sinon-chai": "^2.13.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "chokidar": "^1.7.0",
    "express": "^4.15.4",
    "graceful-fs": "^4.1.11",
    "lodash.debounce": "^4.0.8",
    "tractor-error-handler": "^0.2.2",
    "tractor-logger": "^0.1.0"
  }
}
